{
  "version": 2,
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" },
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "." } }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "src": "/Admin/(.*)", "dest": "/api/admin?path=$1" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
